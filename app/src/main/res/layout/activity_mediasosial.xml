<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:gravity="center_horizontal"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <ImageView
            android:id="@+id/btn_back_medsos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
            <ImageView
                    android:id="@+id/view"
                    android:layout_width="match_parent"
                    android:layout_height="250dp">
            </ImageView>
            <RelativeLayout
                    android:layout_marginHorizontal="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingHorizontal="30dp"
                    android:background="@color/colorSecondary"
                    android:paddingVertical="30dp"
                    android:gravity="center_vertical">
                <ImageView
                        android:layout_centerVertical="true"
                        android:id="@+id/ic_medsos_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:scaleType="fitXY"
                        android:minWidth="20dp"
                        android:minHeight="20dp"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_toRightOf="@id/ic_medsos_phone">
                    <TextView
                            android:id="@+id/tv_medsos_echsan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_echsan"/>
                    <TextView
                            android:layout_below="@id/tv_medsos_echsan"
                            android:id="@+id/tv_medsos_pt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_pt"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_marginHorizontal="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:paddingHorizontal="30dp"
                    android:background="@color/colorSecondary"
                    android:paddingVertical="30dp"
                    android:gravity="center_vertical">
                <ImageView
                        android:layout_centerVertical="true"
                        android:id="@+id/ic_medsos_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:scaleType="fitXY"
                        android:minWidth="20dp"
                        android:minHeight="20dp"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_toRightOf="@id/ic_medsos_email">
                    <TextView
                            android:id="@+id/tv_medsos_email"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_email"/>
                    <TextView
                            android:layout_below="@id/tv_medsos_email"
                            android:id="@+id/email2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_email2"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_marginHorizontal="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:paddingHorizontal="30dp"
                    android:background="@color/colorSecondary"
                    android:paddingVertical="30dp"
                    android:gravity="center_vertical">
                <ImageView
                        android:layout_centerVertical="true"
                        android:id="@+id/ic_medsos_ig"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:scaleType="fitXY"
                        android:minWidth="20dp"
                        android:minHeight="20dp"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_toRightOf="@id/ic_medsos_ig">
                    <TextView
                            android:id="@+id/tv_medsos_ig"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_ig"/>
                    <TextView
                            android:layout_below="@id/tv_medsos_ig"
                            android:id="@+id/ig"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_ig2"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_marginHorizontal="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:paddingHorizontal="30dp"
                    android:background="@color/colorSecondary"
                    android:paddingVertical="30dp"
                    android:gravity="center_vertical">
                <ImageView
                        android:layout_centerVertical="true"
                        android:id="@+id/ic_medsos_facebook"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:scaleType="fitXY"
                        android:minWidth="20dp"
                        android:minHeight="20dp"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_toRightOf="@id/ic_medsos_facebook">
                    <TextView
                            android:id="@+id/facebook"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_facebook"/>
                    <TextView
                            android:layout_below="@id/facebook"
                            android:id="@+id/facebook2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tv_medsos_facebook2"/>
                </LinearLayout>
            </RelativeLayout>


        </LinearLayout>


</RelativeLayout>